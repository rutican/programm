#Создаем таблицу маршрутизации
/routing/table add fib name=SSTP
/ip firewall nat add action=masquerade chain=srcnat out-interface=sstp-laga
/ip/firewall/mangle add chain=prerouting dst-address-list=SSTP action=mark-routing  new-routing-mark=SSTP
/ip/route add gateway=sstp-laga distance=1 routing-table=SSTP
/ip dhcp-client set [find comment="defconf"] add-default-route=special-classless  default-route-distance=2
/ip firewall filter set [find comment="FastTrack"] disabled=yes
/system reboot
Y

=============== удалить все что сделал==========
#/routing/table add fib name=SSTP
routing/table/remove SSTP

#/ip firewall nat add action=masquerade chain=srcnat out-interface=sstp-laga
/ip firewall nat remove [find chain=srcnat out-interface=sstp-laga action=masquerade]

#/ip/firewall/mangle add chain=prerouting dst-address-list=SSTP action=mark-routing  new-routing-mark=SSTP
/ip/firewall/mangle remove [find chain=prerouting dst-address-list=SSTP action=mark-routing  new-routing-mark=SSTP]

#/ip/route add gateway=sstp-laga distance=1 routing-table=SSTP
/ip/route remove [find gateway=sstp-laga distance=1 routing-table=SSTP]

#/ip dhcp-client set [find comment="defconf"] add-default-route=special-classless  default-route-distance=2

#/ip firewall filter set [find comment="defconf: fasttrack"] disabled=yes
/ip firewall filter set [find comment="defconf: fasttrack"] disabled=no



